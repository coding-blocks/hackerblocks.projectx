<div class="t-align-c font-sm mb-5">
  Archived Contests are already ended.
  You can still participate, however you will not be awarded any points for these contest.
</div>
{{#if (not college_contests.length)}}
{{contest-view/empty-contest-screen}}
{{/if}}

<Contest::ContestList
  @page={{page}}
  @fetchContestsTask={{fetchContestsTask}} 
  @buttonText='Solve Challenge'
  as |typed_contests|>
  <PaginationPill 
    @pagination={{typed_contests.meta.pagination}} 
    @setOffset={{action 'setOffset' }} />

</Contest::ContestList>